{
  "manifest_version": 3,
  "name": "Delete Site History",
  "version": "1.0",
  "description": "Delete browsing history of a specific website.",
  "permissions": ["history"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
